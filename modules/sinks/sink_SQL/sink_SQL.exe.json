{
    "sink_sqlscripts":{
        "name":"sql scripts",
        "version":"1,1,0",
        "parameters":{
            "DRIVER":{
                "type":"string",
                "default":"QSQLITE",
                "tooltip":"Qt Database Driver",
                "range":{"tip":"QIBASE,QSQLITE,QMYSQL,QMYSQL3,QODBC,QODBC3,QPSQL,QPSQL7"}
            },
            "HOST":{
                "type":"string",
                "default":"127.0.0.1",
                "tooltip":"server address"
            },
            "PORT":{
                "type":"string",
                "default":1234,
                "tooltip":"server port"
            },
            "DATABASE":{
                "type":"string",
                "default":".\\my.db",
                "tooltip":"database instance"
            },
            "USER":{
                "type":"string",
                "default":"user",
                "tooltip":"user name"
            },
            "PASS":{
                "type":"string",
                "default":"pass",
                "tooltip":"password"
            },
            "hide_window":{
                "type":"int",
                "default":"0",
                "tooltip":"hide the window",
                 "range":{"tip":"0=show,1=hide"}
            },
            "use_transaction":{
                "type":"int",
                "default":"0",
                "tooltip":"use transaction",
                 "range":{"tip":"0=disable transaction,1=enable transaction"}
            },
            "commit_time":{
                "type":"int",
                "default":"1",
                "tooltip":"commit time period(seconds)"
            },
            "encoding":{
                "type":"int",
                "default":"0",
                "tooltip":"char encoding",
                "range":{"tip":"0=936;1=UTF-8"}
            },
            "start_script":{
                "type":"string",
                "default":"",
                "tooltip":"Database start script",
                "range":{"tip":"use this script to start database."}
            }


        },
        "input_subjects":{
            "sql":{
                "type":"byte",
                "tooltip":"sql script input"
            }
        },
        "output_subjects":{
        }
    }
}
